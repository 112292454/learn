import java.util.Scanner;

public class teste {
	public static void main(String[] args) {
		Scanner in=new Scanner(System.in);
		String all=in.nextLine();
		System.out.println("f'(x)=");
		String[] str=all.split("");
		for (int i = 5; i < str.length; i++) {
			int a=1,n=1;
			int xtimes=0;
			if(str[i].equals("x")&&str[i+1].equals("^")) {
				xtimes++;
				String temp="";
				n=1;
				for (int j = i+2; j < str.length; j++) {//得到n――幂
					if(str[j]!="+"&&str[j]!="-") {
						temp+=str[j];
					}else {
						n=new Integer(temp).intValue();
						temp="";
						break;
					}
				}
				
				a=1;
				for (int j = i-1; j < str.length; j--) {
					boolean flag=false;
					if(str[j]=="+"||str[j]=="="&&flag) {
						a=new Integer(temp).intValue();
						break;
					}else if(str[j]=="+"||str[j]=="="&&(!flag)) {
						break;
					}
					else if(str[j]=="-"&&flag) {
						a=new Integer(temp).intValue();
						a=0-a;
						break;
					}else if(str[j]=="-"&&(!flag)) {
						a=-1;
						break;
					}else {
						temp=str[j]+temp;
						flag=true;
					}
				}
				
				a*=n;
				n--;
				if(xtimes!=1) System.out.println(a>0?"+":"");
				System.out.println(a+"x^"+n);
			}else if(str[i]=="x"&&str[i+1]!="^") {
				xtimes++;
				//输出一次项系数
				String temp="";
				for (int j = i-1; j < str.length; j--) {
					boolean flag=false;
					if(str[j]=="+"||str[j]=="="&&flag) {
						a=new Integer(temp).intValue();
						break;
					}else if(str[j]=="+"||str[j]=="="&&(!flag)) {
						break;
					}
					else if(str[j]=="-"&&flag) {
						a=new Integer(temp).intValue();
						a=0-a;
						break;
					}else if(str[j]=="-"&&(!flag)) {
						a=-1;
						break;
					}else {
						temp=str[j]+temp;
						flag=true;
					}
				}
				
				if(xtimes!=1) System.out.println(a>0?"+":"");
				System.out.println(a);
			}
		}
	}
}
